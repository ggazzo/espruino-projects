{
	"name": "tspruino-quickstart",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"start": "turbo run start",
		"clean": "turbo run clean",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"prettier": "^3.0.0",
		"turbo": "latest"
	},
	"packageManager": "yarn@4.5.1"
}
